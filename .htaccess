<IfModule mod_rewrite.c>
    <IfModule mod_negotiation.c>
        Options -MultiViews
    </IfModule>

    RewriteEngine On

    # Chuyển hướng tất cả các yêu cầu tới thư mục public
    RewriteRule ^(.*)$ public/$1 [L]
</IfModule>
